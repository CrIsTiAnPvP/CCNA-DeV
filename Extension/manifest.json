{
    "manifest_version": 3,
    "name": "CCNA ðŸ˜‰ DeV",
    "version": "6.9",
    "description": "CCNA Coded By CrIsTiiAnPvP ðŸ˜Ž",
    "permissions": [
        "contextMenus",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css": ["popup.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["respuesta.json"],
            "matches": ["<all_urls>"]
        }
    ]
}